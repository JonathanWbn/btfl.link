{
  "version": 2,
  "builds": [
    { "src": "next.config.js", "use": "@now/next" },
    { "src": "api/*.js", "use": "@now/node" }
  ],
  "routes": [
    { "src": "/redirect", "methods": ["POST"], "dest": "api/post-redirect.js" },
    { "src": "/[a-z0-9-]+", "methods": ["GET"], "dest": "api/get-redirect.js" }
  ]
}
